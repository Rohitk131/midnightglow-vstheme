{
	"name": "Cyberpunk Midnight Zen",
	"colors": {
	  "editor.background": "#1A1A2E",
	  "editor.foreground": "#F0F0F0",
	  "activityBarBadge.background": "#FF6B9D",
	  "sideBarTitle.foreground": "#00BFFF",
	  "titleBar.activeBackground": "#3C3C5A",
	  "statusBar.background": "#67506e",
	  "sideBar.background": "#1B1B2D",
	  "activityBar.background": "#121212",
	  "tab.activeBackground": "#2A2A6D",
	  "tab.inactiveBackground": "#1B1B4D",
	  "editorGroupHeader.tabsBackground": "#121212",
	  "list.activeSelectionBackground": "#4E4E78",
	  "list.hoverBackground": "#037062",
	  "panel.background": "#1A1A2E",
	  "terminal.background": "#0F0F1E",
		"terminal.foreground": "#ffe0e0",
		"terminalCursor.background": "#33f702",
		"terminalCursor.foreground": "#1eff00",
		"terminal.ansiBlack": "#121212",
		"terminal.ansiBlue": "#40C9FF",
		"terminal.ansiBrightBlack": "#666666",
		"terminal.ansiBrightBlue": "#80D9FF",
		"terminal.ansiBrightCyan": "#80FFFF",
		"terminal.ansiBrightGreen": "#80FFE0",
		"terminal.ansiBrightMagenta": "#f551a0",
		"terminal.ansiBrightRed": "#FF9E9E",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#FFFF80",
		"terminal.ansiCyan": "#00FFFF",
		"terminal.ansiGreen": "#00FFDB",
		"terminal.ansiMagenta": "#FF6B9D",
		"terminal.ansiRed": "#FF4040",
		"terminal.ansiWhite": "#E0E0FF",
		"terminal.ansiYellow": "#00ff88",
		"terminal.selectionBackground": "#4E4E78"
	
	},
	"tokenColors": [
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#5077a5"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": {
		  "foreground": "#ffd0fb"
		}
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": {
		  "foreground": "#ffffff"
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#FF6B9D"
		}
	  },
	  {
		"name": "Keyword, Storage",
		"scope": ["keyword", "storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#00FFDB"
		}
	  },
	  {
		"name": "Operator, Misc",
		"scope": [
		  "keyword.control", 
		  "constant.other.color", 
		  "punctuation", 
		  "meta.tag", 
		  "punctuation.definition.tag", 
		  "punctuation.separator.inheritance.php", 
		  "punctuation.definition.tag.html", 
		  "punctuation.definition.tag.begin.html", 
		  "punctuation.definition.tag.end.html", 
		  "punctuation.section.embedded", 
		  "keyword.other.template", 
		  "keyword.other.substitution"
		],
		"settings": {
		  "foreground": "#00FFFF"
		}
	  },
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#FF6B9D"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#40C9FF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#FF6B9D"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF6B9D"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FFB16D"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#7DFFCD"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFB16D"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#4DFFF3"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#4DFFF3"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF6B9D"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF6B9D"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#40C9FF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#40C9FF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#00FFDB"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFB16D"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
