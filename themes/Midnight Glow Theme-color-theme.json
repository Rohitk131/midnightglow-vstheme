			],
			"settings": {
				"foreground": "#FF6B9D"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#00FFDB"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
